staff 3
	visible=n
staff 4
	stafflines = tab(c d)

staff 5
	visible=n
staff 6
	stafflines = tab(c d e)

staff 7
	visible=n
staff 8
	stafflines = tab(c d e f)

staff 9
	visible=n
staff 10
	stafflines = tab(c d e f g)

staff 11
	visible=n
staff 12
	stafflines = tab(c d e f g a)

staff 13
	visible=n
staff 14
