	title ""
score
	staffs=2
	endingstyle=top
	packfact=1.1
	aboveorder=ending,othertext
	scoresep=10,28


music

rom above all: 1 "endingstyle=top";
1: [cue] 4ce; 4g;  4f; e;
bar ending "first"
1:  2df;  {4c;e;f;}3;
bar ending "1."
1:  2df; ;
bar
1: 4f;;;;
bar ending "2."
1: 4bd+g+b++ ;;;;
bar ending "3."
1: 8ce;;;;;;;;
endbar endending

newscore

score
	endingstyle=grouped
	bracket=1,2

