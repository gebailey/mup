1-2: e;r;f;r;
repeatstart

1-2: e;g;a;b;
bar ending "1."

1-2: f;b;a;g;
repeatend ending "2."

1-2: g;f;e;c;
rom above all: 1 "closedendending";
bar closedendending

1-2: 2c;;
endbar
